    "prettier": "^2.7.1",
    "typescript": "^4.7.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.16.1",
    "@types/node": "^18.7.8",
    "concurrently": "^4.1.0",
    "nodemon": "^1.19.0",
    "tslint": "^6.1.3",
    "typescript": "^3.4.5"
